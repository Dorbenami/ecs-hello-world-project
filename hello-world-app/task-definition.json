{
    "family": "hello-world-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "hello-world",
        "image": "REPLACE_ME",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::381492011511:role/ecsTaskExecutionRole"
  }
  